#
# RUN: fab --format=null --print-dag %s > %t
# RUN: FileCheck %s -input-file %t
#

# CHECK-DAG: file foo = foo.c
foo = file('foo.c');

# CHECK-DAG: file bar = custom-subdir/bar.c
bar = file('bar.c', subdir = 'custom-subdir');

# CHECK-DAG: list[file] baz = [ custom2/baz1.c custom2/baz2.c ]
baz = files(baz1.c baz2.c, subdir = 'custom2');

# CHECK-DAG: file wibble = custom3/wibble.c
wibble = {
	subdir = 'custom3';
	file('wibble.c')
};
