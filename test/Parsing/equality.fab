#
# RUN: %fab --parse-only --print-ast %s > %t
# RUN: FileCheck %s -input-file %t
#

x = true;
y = false;

# CHECK: equal:bool = x == y;
equal = (x == y);

# CHECK: nequal:bool = x != y;
nequal = (x != y);

# CHECK: notequal:bool = not x == y;
notequal = not x == y;

hello = 'Hello';
world = 'World';

# CHECK: strequal:bool = hello == world;
strequal = (hello == world);

# CHECK: strnequal:bool = hello != world;
strnequal = (hello != world);

# CHECK: strnotequal:bool = not hello == world;
strnotequal = not hello == world;
