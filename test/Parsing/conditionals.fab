# CHECK: foo:bool = true;
foo = true;

# CHECK: bar:int = if (foo) { 42 } else { 314 };
bar = if foo 42 else 314;

# CHECK: baz:string = if (not foo) { 'hello' } else { 'world' };
baz = if not foo "hello" else "world";
