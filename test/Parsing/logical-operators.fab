# CHECK: foo:bool = true;
foo = true;

# CHECK: bar:bool = not foo;
bar = not foo;

# CHECK: baz:bool = foo or bar;
baz = foo or bar;

# CHECK: wibble:bool = foo and bar;
wibble = foo and bar;

# CHECK: wobble:bool = foo xor bar;
wobble = foo xor bar;
