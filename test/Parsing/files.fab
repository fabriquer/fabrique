#
# RUN: fab --parse-only --print-ast %s > %t
# RUN: FileCheck %s -input-file %t
#

# CHECK: foo:file = file('Inputs/foo.txt');
foo = file('Inputs/foo.txt');

# CHECK: bar:list[file] = [ file('Inputs/foo.txt') ];
bar = files(     Inputs/foo.txt     );

# CHECK: baz:list[file] = [ file('Inputs/foo.txt') file('Inputs/baz.txt') ];
baz = files(
	Inputs/foo.txt
	file('Inputs/baz.txt')
);

# CHECK: wibble:list[file] = [ file('wibble.txt'), subdir = 'Inputs' ];
wibble = files(wibble.txt, subdir = 'Inputs');
