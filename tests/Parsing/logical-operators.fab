# CHECK: foo = true;
foo = true;

# CHECK: bar:bool = not foo;
bar:bool = not foo;

# CHECK: baz = foo or bar;
baz = foo or bar;

# CHECK: wibble:bool = foo and bar;
wibble:bool = foo and bar;

# CHECK: wobble = foo and bar or baz xor wibble;
wobble = foo and bar or baz xor wibble;
