#
# RUN: %fab --print-dag --format=null %s > %t
# RUN: FileCheck %s -input-file %t
#

cc = action('cc -c ${src} -o ${obj}' <- src:file[in], obj:file[out]);

# CHECK-DAG: foo:file = cc { foo.c => foo.o }
foo = cc(file('foo.c'), file('foo.o'));

# CHECK-DAG: name:string = 'foo.o'
name = foo.filename;
